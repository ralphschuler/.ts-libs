{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "yarn:foreach": "yarn workspaces foreach --exclude @lib-lib/random --exclude @lib-lib/signature -Apt",
    "publish": "yarn yarn:foreach run publish",
    "prepare": "yarn yarn:foreach run prepare",
    "build": "yarn yarn:foreach run build",
    "test": "yarn yarn:foreach run test",
    "lint": "yarn yarn:foreach run lint",
    "lint:fix": "yarn yarn:foreach run lint:fix",
    "format": "yarn yarn:foreach run format",
    "clean": "yarn yarn:foreach run clean",
    "prepublish": "yarn build",
    "start": "tsx src/index.ts"
  },
  "packageManager": "yarn@4.0.2",
  "dependencies": {
    "tsx": "^4.1.4"
  }
}
